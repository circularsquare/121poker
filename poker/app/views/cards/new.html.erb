<h1> make a card! </h1>

<%= form_with scope: :card, url: cards_path, local: true do |form| %>

  <p>
    <%= form.label :suit %><br>
    <%= form.text_field :suit %>
  </p>

  <p>
    <%= form.label :rank %><br>
    <%= form.text_field :rank %>
  </p>

  <p>
    <%= form.submit %>
  </p>
<% end %>